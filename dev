PROJ="/proj/jenov/scripts/ymake_dev"
LIB="/proj/jenov/scripts/y_functions.sh"
DEST1="/usr/local/bin"
DEST2="/bin"

#DEF1="--nosync --noupdate --nocheck --nobuild --notest --nomerge -D -N"
DEF1="--nosync --noupdate --nocheck --nobuild -notest --nomerge -lkernel -ckernel -bkernel -lilo -rebuild -cgentoo -D"

cp $PROJ $DEST1
cp $LIB $DEST2
$DEST1/ymake_dev $DEF1 $*
fsck -p /dev/sdb3 2>&1 >> /var/log/ymake_dev.log
